    "    plt.ylabel('Amplitude')\n",
    "    plt.title('Differential Manchester Encoding')\n",
    "    plt.show()\n",
    "    return\n",
    "\n",
    "def AMI(dataStream):\n",
    "    d=[]\n",
    "    amp=-4\n",
